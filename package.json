{
  "name": "groupmedarktheme",
  "version": "1.1.0",
  "description": "Dark theme for the GroupMe web application",
  "scripts": {
    "build": "npx tsc && node-sass ./scss/styles.scss ./dist/styles.css",
    "ts-watch": "tsc -w",
    "scss-watch": "sass --no-source-map --watch scss/styles.scss:dist/styles.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akump/GroupMeDarkTheme.git"
  },
  "author": "Andrew Kump",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akump/GroupMeDarkTheme/issues"
  },
  "homepage": "https://github.com/akump/GroupMeDarkTheme#readme",
  "prettier": "prettier-airbnb-config",
  "dependencies": {
    "@types/chrome": "^0.0.133",
    "node-sass": "^5.0.0",
    "sass": "^1.32.6",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "prettier-airbnb-config": "^1.0.0"
  }
}
