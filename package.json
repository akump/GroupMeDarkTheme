{
    "name": "groupmedarktheme",
    "version": "1.2.11",
    "description": "Dark theme for the GroupMe web application",
    "scripts": {
        "build": "npx tsc && sass ./scss/styles.scss ./dist/styles.css && gulp moveHtml && gulp moveJs",
        "build-zip": "gulp",
        "ts-watch": "tsc -w",
        "scss-watch": "sass --no-source-map --watch scss/styles.scss:dist/styles.css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/akump/GroupMeDarkTheme.git"
    },
    "author": "Andrew Kump",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/akump/GroupMeDarkTheme/issues"
    },
    "homepage": "https://github.com/akump/GroupMeDarkTheme#readme",
    "prettier": "prettier-airbnb-config",
    "type": "module",
    "dependencies": {
        "@types/chrome": "^0.0.133",
        "gulp": "^4.0.2",
        "gulp-zip": "^5.1.0",
        "sass": "^1.32.6",
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^5.47.1",
        "eslint": "^8.31.0",
        "node-sass": "npm:sass@^1.45.2",
        "prettier": "^1.19.1",
        "prettier-airbnb-config": "^1.0.0"
    }
}