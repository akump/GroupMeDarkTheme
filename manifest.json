{
  "manifest_version": 2,
  "name": "GroupMe Dark Theme",
  "version": "1.1.1",
  "description": "A chrome extension to support a dark theme for GroupMe's website",
  "content_scripts": [{
    "matches": ["https://web.groupme.com/*"],
    "js": ["dist/script.js"],
    "css": ["dist/styles.css"]
  }],
  "icons": {
    "16": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_popup": "dist/settings.html"
  },
  "permissions": [
    "storage"
  ]
}