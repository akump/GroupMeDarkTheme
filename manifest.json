{
    "manifest_version": 3,
    "name": "GroupMe Dark Theme",
    "version": "1.2.13",
    "description": "A chrome extension to support a dark theme for GroupMe's website",
    "content_scripts": [
        {
            "matches": [
                "https://web.groupme.com/*"
            ],
            "js": [
                "dist/script.js",
                "dist/button.prod.min.js"
            ],
            "css": [
                "dist/styles.css"
            ]
        }
    ],
    "icons": {
        "16": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_popup": "dist/settings.html"
    },
    "permissions": [
        "storage"
    ]
}